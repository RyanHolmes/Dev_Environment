<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Your Username</title>
	<link rel="stylesheet" href="../stylesheets/profile.css">
	<link rel="stylesheet" href="../stylesheets/bootstrap.min.css">
	<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.js"></script>
	<script src="javascripts/profile.js"></script>
</head>
<body>
	<!-- NAV BAR -->
	<div class="row">
        <div class="col-lg-12" id="landingNavbar">
        	<div class="row">
        		<div class="col-lg-1">
		            <img src="images/logo.png" alt="" id="landingNavbarImg">
		        </div>

		       	<div class="col-lg-2 col-lg-offset-7">
		            <div class="input-group input-group-lg" id="landingSearchbar">
		            	<input type="text" class="form-control" placeholder="Search for...">
		            	<span class="input-group-btn">
		              		<button class="btn btn-default" type="button">Go!</button>
		            	</span>
		            </div>
		        </div>
				
				<div class="col-lg-1">
		        	<span id="landingLoginBtn">Log-In</span>
		        </div>
		        <div class="col-lg-1">
            		<span id="landingSignupBtn"><a href="http://localhost:3000/signup">Sign-Up</a></span>
            	</div>
            </div>
        </div>
    </div>
    <!-- TAB PANEL BUTTONS FOR PRFILE - PROFILE, CALENDAR, FAVOURITES/LISTINGS, PREMIUM  -->
    <div class="row" id="profileTabPanel">
    	<div class="col-lg-2 col-lg-offset-2 profileTabBtn" id="profileTabBtnPro"><h4>Profile</h4></div>
    	<div class="col-lg-2 profileTabBtn" id="profileTabBtnCal"><h4>Calendar</h4></div>
    	<div class="col-lg-2 profileTabBtn" id="profileTabBtnFav"><h4>Favourites</h4></div>
    	<div class="col-lg-2 profileTabBtn" id="profileTabBtnSet"><h4>Settings</h4></div>
    	<div class="col-lg-2 profileTabBtn" id="profileTabBtnPre"><h4>Premium</h4></div>
    </div>
    <!-- profile tab container -->
    <div class="row" id="profileTab">
    	<!-- show/hide html here based on clicked tab -->
    	<div class="profileTabItem" id="tabPro">
    		<p>Profile Tab</p>
    	</div>
    	<div class="profileTabItem" id="tabCal">
    		<p>Calendar Tab</p>
    	</div>
    	<div class="profileTabItem" id="tabFav">
    		<p> Favourites Tab</p>
    	</div>
    	<div class="profileTabItem" id="tabSet">
    		<p> Settings Tab</p>
    	</div>
    	<div class="profileTabItem" id="tabPre">
    		<p> Premium Upgrade Tab</p>
    	</div>
    </div>
</body>
</html>